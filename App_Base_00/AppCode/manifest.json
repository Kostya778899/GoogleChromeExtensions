{
  "manifest_version": 3,
  "name": "App base (test)",
  "version": "1.0.0",
  "description": "It is app base test.",
  "icons": { "128": "/images/app_icon/128.png" },

  "content_security_policy": {
    //"script-src 'self' 'unsafe-eval'; object-src 'self';"

    //"script-src": [ "self", "wasm-unsafe-eval" ],
    //"object-src": [ "self", "unsafe-eval" ]

    //"extension_pages": "script-src 'self' 'wasm-eval'; object-src 'self'"

    "script-src": "unsafe-eval"

    //"default-src": "unsafe-eval"

    //"script-src 'self'; object-src 'none'"

  },
  //"content_security_policy": {
  //  "extension_pages": "self",
  //  "sandbox": "self"
  //},

  "action": {
    "default_popup": "/popup/index.html",
    "default_icon": { "128": "/images/app_icon/128.png" }
  },

  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "background.js", "app.js" ]
    }
  ],

  "web_accessible_resources": [
    {
      "matches": [ "<all_urls>" ],
      "resources": [ "background.js", "topbar.html" ]
    }
  ],

  "sandbox": {
    "pages": [ "sandbox.html" ]
  },

  "permissions": [
    "tabs",
    "background",
    "clipboardWrite", //---
    "scripting"
  ],
  "host_permissions": [ "<all_urls>" ]
}


