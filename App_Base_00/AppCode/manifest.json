{
  "manifest_version": 3,
  "name": "App base (test)",
  "version": "1.0.0",
  "description": "It is app base test.",
  "icons": { "128": "/images/app_icon/128.png" },

  "content_security_policy": {
    "object-src": [ "unsafe-eval" ]
  },

  "action": {
    "default_popup": "/popup/index.html",
    "default_icon": { "128": "/images/app_icon/128.png" }
  },

  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "run_at": "document_start",
      "js": [ "background.js", "databaseLoader/main.js" ]
    },
    //{
    //  "matches": [ "<all_urls>" ],
    //  "run_at": "document_end",
    //  "js": []
    //},
    {
      "matches": [ "<all_urls>" ],
      "run_at": "document_idle",
      "js": [ "app.js" ]
    }
  ],

  "web_accessible_resources": [
    {
      "matches": [ "<all_urls>" ],
      "resources": [ "/databaseLoader/unsafe.js" ]
    }
  ],

  "sandbox": {
    "pages": [ "sandbox.html" ]
  },

  "permissions": [
    "tabs",
    "background",
    "clipboardWrite", //---
    "scripting"
  ],
  "host_permissions": [ "<all_urls>" ]
}


