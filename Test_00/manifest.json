{
   "manifest_version": 3,
   "name": "Mail link healer",
   "version": "1.0.0",
   "description": "Ссылка «Входящие» и «Написать письмо» снова открываются в той же вкладке!",
   "icons": {
       "128": "128.png"
   },

   "content_scripts": [
       {
           "matches": [ "<all_urls>" ],
           "js": [ "app.js" ]
       }
   ],

   "web_accessible_resources": [
    {
      "resources": [ "128.png", "logo_index.html" ],
      "matches": [ "https://www.youtube.com/*" ]
    }


  ]
}